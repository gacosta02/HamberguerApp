Create DataBase HamBurguerApp

Create Table Burgers(
Id integer Primarykey,
Picture Varchar(100),
Name Varchar(20),
Descripcion Varchar(200) ,
Rating Varchar (50) ,
RatingDetail Varchar (100) ,
Like Varchar (20)

)
Create Table Usuarios (
Id integer Primarykey,
Nombre Varchar(20),
Direccion Varchar(200),
Celular Varchar(15) ,
IDTarjetaCredito integer

)
